<%= simple_form_for(@designer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :givenname %>
    <%= f.input :school %>
    <%= f.input :linkedin %>
    <%= f.input :instagram %>
    <%= f.input :facebook %>
    <%= f.input :website %>
    <%= f.input :profilepic %>
    <%= f.input :universpics %>
    <%= f.input :interview %>
    <%= f.input :universvid %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
